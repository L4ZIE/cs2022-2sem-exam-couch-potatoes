               </styleClass></Button>
            <Button fx:id="privateProjectsBtn" mnemonicParsing="false" onAction="#privateProjectsBtnPressed" prefHeight="26.0" prefWidth="137.0" stylesheets="@../css/MainStyle.css" text="Private Projects">
