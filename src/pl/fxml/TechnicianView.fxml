                  <String fx:value="common-text-font" />
               </styleClass></Button>
            <Button fx:id="sendBtn" focusTraversable="false" mnemonicParsing="false" onAction="#sendBtnPressed" prefWidth="125.0" stylesheets="@../css/MainStyle.css" text="Send As Email">
